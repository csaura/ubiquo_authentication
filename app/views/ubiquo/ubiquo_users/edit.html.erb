<h1><%= render :partial => 'title' %></h1>
<h2><%= render :partial => "submenu" %></h2>

<%= error_messages_for 'ubiquo_user' %>

<%= ubiquo_form_for @ubiquo_user, :html => { :multipart => true } do |form| %>
  <%= render :partial => 'form', :locals => { :form => form } %>
  <%= form.submit_group do %>
    <%= form.update_button %>
    <%= form.back_button %>
  <% end %>
<% end %>

<% content_for :sidebar do %>
  <%= help_block_sidebar(t("ubiquo.helpmsg.authentication.ubiquousers.edit")) %>
<% end %>
