<h1><%= render :partial => 'title' %></h1>
<h2>
  <%= render :partial => "submenu" %>
</h2>
<%= render(:partial => "shared/ubiquo/feedback") %>

<% form_for([:ubiquo, @ubiquo_user]) do |form| %>
	<%= render :partial => 'form', :locals => {:form => form} %>
	  <p class="form_buttons">
	    <%= form.submit t("ubiquo.create") %>
			<%= button_to_function t("ubiquo.back_to_list"), "document.location.href='#{ubiquo_ubiquo_users_path}'" %>
	  </p>
<% end %>

<% content_for :sidebar do %>
	<%= help_block_sidebar(t("ubiquo.helpmsg.authentication.ubiquousers.new")) %>
<% end %>
